<div class="row sub-header">
  <div class="col-md-12">
    <h5><a href="<%= admin_admins_path %>">Admins&gt;&gt;</a> Edit</h5>
  </div>
</div>

<%= simple_form_for @admin, url: admin_admin_path do |f| %>
  <%= f.input :email, disabled: true %>
  <%= f.input :first_name, type: :text %>
  <%= f.input :last_name, type: :text %>
  <%= f.input :phone, type: :text %>
  <%= f.submit "Update", class: "btn btn-primary"%>
<% end %>
